{
  "kind": "collectionType",
  "collectionName": "books",
  "info": {
    "singularName": "book",
    "pluralName": "books",
    "displayName": "book",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "thumbnail": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "price": {
      "type": "integer",
      "required": true
    },
    "book_id": {
      "type": "uid",
      "required": true
    },
    "author": {
      "type": "string",
      "required": true
    },
    "genre": {
      "type": "string"
    },
    "publication": {
      "type": "string"
    },
    "description": {
      "type": "text",
      "required": true
    },
    "compare_at_price": {
      "type": "integer"
    },
    "stock": {
      "type": "integer"
    },
    "orders": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::order.order",
      "mappedBy": "books"
    },
    "seo": {
      "displayName": "seoTitle",
      "type": "component",
      "repeatable": false,
      "component": "seo.seo-title"
    },
    "wishlists": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::wishlist.wishlist",
      "mappedBy": "books"
    },
    "likes": {
      "type": "json"
    }
  }
}
